! Dracula Xresources palette
*foreground: #F8F8F2
*background: #282A36
*color0:     #000000
*color8:     #4D4D4D
*color1:     #FF5555
*color9:     #FF6E67
*color2:     #50FA7B
*color10:    #5AF78E
*color3:     #F1FA8C
*color11:    #F4F99D
*color4:     #CAA9FA
*color12:    #CAA9FA
*color5:     #FF79C6
*color13:    #FF92D0
*color6:     #8BE9FD
*color14:    #9AEDFE
*color7:     #BFBFBF
*color15:    #E6E6E6

! Xft settings
Xft*dpi: 96
Xft*antialias: true
Xft*rgba: rgb
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*autohint: false
Xft*lcdfilter: lcddefault

! URxvt settings
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollBar_floating: false
URxvt*scrollstyle: rxvt
URxvt*cursorBlink: true

URxvt*font: xft:DejaVu Sans Mono:size=10
URxvt*boldFont: xft:DejaVu Sans Mono:bold:size=10
URxvt*italicFont: xft:DejaVu Sans Mono:italic:size=10
URxvt*boldItalicFont: xft:DejaVu Sans Mono:bold:italic:size=10

! rofi settigs
! Enable the extended coloring options
rofi.color-enabled: true

!                  bg       border   separator
rofi.color-window: #272935, #bc92f8, #272935

!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #272935, #f7f7f1, #272935, #fe5454, #272935
rofi.color-active: #272935, #f7f7f7, #272935, #fe5454, #272935
rofi.color-urgent: #272935, #f7f7f7, #272935, #fe5454, #272935

rofi*font: DejaVu Sans Book 10

rofi*matching: fuzzy
